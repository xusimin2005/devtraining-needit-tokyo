        <script><![CDATA[function onClick() {
	var message0 = "请选择要比较的记录";
	var message1 = "请选择另一条要比较的记录";
	var message2 = "请选择二条要比较的记录";

	var selected = g_list.getChecked();
	if (!selected) {
		alert(message0);
		return;
	}

	var selects = selected.split(",");
	if (selects.length == 1) {
		alert(message1);
		return;
	}
	if (selects.length > 2) {
		alert(message2);
		return;
	}

	//alert(selected);

	var gu = new GlideURL('x_58872_needit_sample_page.do');
	gu.addParam('sysparm_fullname', '老李');
	gu.addParam('select1', selects[0]);
	gu.addParam('select2', selects[1]);
	g_navigation.open(gu.getURL());
	//action.setRedirectURL('x_58872_needit_sample_page.do'); //action只能用在server端
}
]]></script>
